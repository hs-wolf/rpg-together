{
  "name": "client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/client/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi build ./apps/client"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi dev ./apps/client"
      }
    },
    "generate-pages": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi generate ./apps/client"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi preview ./dist/apps/client"
      }
    },
    "prepare": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nuxi prepare ./apps/client"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx eslint --fix ./apps/client"
      }
    }
  }
}
